package Arrays;

public class Doors {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		boolean[] doors = new boolean[100];
		
		for (int i = 0; i < doors.length; i++) {
			for (int j = i; j < doors.length; j += i) {
				doors[j] = !doors[j];
			}
		}
		
		for (int i = 0; i < doors.length; i++) {
			switch (doors[i]) {
			case true: break;
			case false: break;
			default: break;
			}
			if (doors[i]) {
				System.out.println("Door " + (i+1) + " is open.");
			}
			else if (doors[i])
		}
	}

}
